<div class="tab-content col-lg-12" id="myTabContent">
    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">

    </div>
    <div class="tab-pane fade" id="settings" role="tabpanel" aria-labelledby="profile-tab">
        <hr>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"><strong> <i class="fas fa-signature"></i>
                            {{language.userInfo.FirstAndLastName}}</strong>
                    </span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.FirstName}}">
                <input type="text" aria-label="Last name" class="form-control"
                    placeholder="{{language.userInfo.Lastname}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"> <strong><i class="fas fa-envelope"></i>
                            {{language.userInfo.ContactInfo}}</strong>
                    </span>
                </div>
                <input type="text" aria-label="Last name" class="form-control"
                    placeholder="{{language.userInfo.cellphone}}">
                <input type="text" aria-label="Last name" class="form-control"
                    placeholder="{{language.signCommentEmail}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"> <strong><i class="fas fa-briefcase"></i>
                            {{language.userInfo.Work}}</strong>
                    </span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.WorkSite}}">
                <input type="text" aria-label="Last name" class="form-control"
                    placeholder="{{language.userInfo.Enterprise}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"> <strong><i class="fas fa-map-marked-alt"></i>
                            {{language.userInfo.Location}}</strong> </span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.Country}}">
                <input type="text" aria-label="Last name" class="form-control" placeholder="{{language.userInfo.City}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"> <strong><i class="fas fa-key"></i>
                            {{language.userInfo.password}}</strong></span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.ChangePassword}}">
                <input type="text" aria-label="Last name" class="form-control"
                    placeholder="{{language.userInfo.confirmPassword}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"><strong><i class="fab fa-github-square"></i>
                            Github</strong></span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.YourGithub}}">
            </div>
        </div>
        <div class="form-group">
            <div class="md-form input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text md-addon"><strong><i class="fa fa-link fa-1x"></i>
                            Webpage</strong></span>
                </div>
                <input type="text" aria-label="First name" class="form-control"
                    placeholder="{{language.userInfo.YourWebPage}}">
            </div>
        </div>
        <button class="btn btn-success" id="saveSettings">
            {{language.userInfo.SaveSettings}}
        </button>
        <hr>

        <div class="custom-control custom-checkbox custom-control-inline">
            <input type="checkbox" class="custom-control-input" id="defaultInline1">
            <label class="custom-control-label" for="defaultInline1">
                <p> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{language.userInfo.ShowPublicName}}</p>
            </label>
        </div>
        <div class="custom-control custom-checkbox custom-control-inline">
            <input type="checkbox" class="custom-control-input" id="defaultInline2">
            <label class="custom-control-label" for="defaultInline2">
                <p> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{language.userInfo.ShowPublicEmail}}</p>
            </label>
        </div>
        <div class="custom-control custom-checkbox custom-control-inline">
            <input type="checkbox" class="custom-control-input" id="defaultInline3">
            <label class="custom-control-label" for="defaultInline3">
                <p> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{language.userInfo.ShowPublicLocation}}</p>
            </label>
        </div>
    </div>
    <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">...
    </div>
</div>